<div class="container">
    <br>
    <h1>새로운 게시글 작성</h1>
    <form action="/board/create" method="POST">
      <div class="form-group">
        <label for="exampleInputEmail1">title</label>
        <input type="text" name="title" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="제목을 입력하시오">
      </div>
      <div class="form-group">
        <label for="exampleInputPassword1">editor</label>
        <input type="text" name="editor" class="form-control" id="exampleInputPassword1" placeholder="작성자">
      </div>
      <div class="form-group">
          <label for="comment">content</label>
          <textarea class="form-control" name="content" rows="10" id="comment"></textarea>
      </div>
      <button type="submit" class="btn btn-primary">작성</button>
    </form>
</div>
